{
  "$schema": "./node_modules/@skyux/config/skyuxconfig-schema.json",
  "name": "stache2",
  "mode": "easy",
  "compileMode": "aot",
  "auth": false,
  "plugins": [
    "@blackbaud/skyux-builder-plugin-stache",
    "@skyux-sdk/builder-plugin-skyux"
  ],
  "app": {
    "title": "Stache 2"
  },
  "omnibar": {
    "experimental": true,
    "nav": {
      "services": [
        {
          "title": "Stache 2",
          "items": [
            {
              "title": "Home",
              "route": "/"
            },
            {
              "title": "Demos",
              "route": "/demos"
            }
          ]
        }
      ]
    }
  },
  "appSettings": {
    "stache": {
      "editButton": {
        "url": "https://github.com/blackbaud/stache2",
        "text": "Suggest Edits"
      },
      "searchConfig": {},
      "footer": {
        "nav": {
          "items": [
            {
              "title": "Home",
              "route": "/"
            },
            {
              "title": "Demos",
              "route": "/demos"
            },
            {
              "title": "API Terms of Use",
              "route": "https://developer.sky.blackbaud.com/terms"
            }
          ]
        }
      },
      "googleAnalytics": {
        "enabled": "true",
        "clientId": "UA-2418840-1",
        "tagManagerContainerId": "GTM-W56QP9"
      }
    }
  },
  "testSettings": {
    "unit": {
      "browserSet": "speedy"
    }
  }
}
